<Window x:Class="CareAmarillo.Page4"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CareAmarillo"
        mc:Ignorable="d"
        Title="Page4" Height="800" Width="800">
    <Grid>
        <Image HorizontalAlignment="Left" Height="170" VerticalAlignment="Top" Width="171" Source="Capture.JPG"/>
        <Button x:Name="button" Content="Back To Main" HorizontalAlignment="Left" Margin="341,688,0,0" VerticalAlignment="Top" Width="104" Height="44" FontSize="14" Background="#FF00C5FF" Foreground="White" Click="Button_Click" FontWeight="Bold" />
        <TextBox Height="44" HorizontalAlignment="Left" Margin="398,158,0,0"
            Name="txtNameToSearch" TextChanged="txtNameToSearch_TextChanged"
        VerticalAlignment="Top" Width="252" Text="Search Shelter Name . . ." FontSize="18" />
        <TextBlock HorizontalAlignment="Left" Margin="171,49,0,0" TextWrapping="Wrap" Text="CareAmarillo" VerticalAlignment="Top" FontSize="36"/>
        <Button x:Name="btnSearchShelter" Content="Search" HorizontalAlignment="Left" Margin="678,158,0,0" VerticalAlignment="Top" Width="104" Height="44" Background="#FF75BC03" FontSize="18" FontWeight="Bold" Click="Button_Click_1" />
        <Button x:Name="btnClearAll" Content="Clear" HorizontalAlignment="Left" Margin="678,227,0,0" VerticalAlignment="Top" Width="104" Height="44" Background="#FF75BC03" FontSize="18" FontWeight="Bold" Click="Button_Click_2" />
        <Label x:Name="lblResult" HorizontalAlignment="Left" Margin="398,227,0,0" VerticalAlignment="Top" Height="314" Width="252"/>
        <Label x:Name="lblWelcomeText" Content="Welcome back, " Margin="493,10,139,711" VerticalContentAlignment="Center" FontSize="18" FontWeight="Bold"/>
        <Label x:Name="lblWelcomeText_Copy" Content="First Name" VerticalContentAlignment="Center" FontSize="18" FontWeight="Bold" Margin="637,10,-5,711"/>
    </Grid>
</Window>

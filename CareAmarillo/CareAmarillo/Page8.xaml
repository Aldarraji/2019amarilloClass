<Page x:Class="CareAmarillo.Page8"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:CareAmarillo"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Page8"
     Height="800" Width="800">
    <Grid Background="White">
        <Image x:Name="imgLogo" Margin="265,27,275,597" Source="Capture.JPG"/>
        <Label x:Name="lblCAp2" Content="CareAmarillo" HorizontalAlignment="Left" Margin="266,198,0,0" VerticalAlignment="Top" FontSize="45"/>
        <Label x:Name="lblFNameP2" HorizontalAlignment="Left" Margin="198,304,0,0" VerticalAlignment="Top" FontSize="20">
            <Label x:Name="lblFNameP2_Copy2" HorizontalAlignment="Left" VerticalAlignment="Top" FontSize="20">
                <Label x:Name="lblFNameP2_Copy3" Content="Click here to change your password" HorizontalAlignment="Left" VerticalAlignment="Top" FontSize="20"/>
            </Label>
        </Label>
        <Label x:Name="lblFNameP2_Copy" Content="Click here to disable or cancel account" HorizontalAlignment="Left" Margin="183,366,0,0" VerticalAlignment="Top" FontSize="20"/>
        <Label x:Name="lblFNameP2_Copy1" Content="View Shelter Data" HorizontalAlignment="Left" Margin="265,418,0,0" VerticalAlignment="Top" FontSize="20" Foreground="#FF7FC769"/>
        <Button x:Name="btnLogin" Content="Change Password" HorizontalAlignment="Left" Margin="559,315,0,0" VerticalAlignment="Top" Width="113" BorderBrush="#FF00C5FF" Background="White" Height="39" FontSize="14" Foreground="#FF00C5FF" Click="BtnLogin_Click"/>
        <Button x:Name="btnLogin_Copy" Content="Account management" HorizontalAlignment="Left" Margin="559,367,0,0" VerticalAlignment="Top" Width="113" BorderBrush="#FF00C5FF" Background="White" Height="39" FontSize="14" Foreground="#FF00C5FF" Click="BtnLogin_Click"/>
        <Button x:Name="btnLogin_Copy1" Content="Shelter Data" HorizontalAlignment="Left" Margin="559,419,0,0" VerticalAlignment="Top" Width="113" BorderBrush="#FF00C5FF" Background="White" Height="39" FontSize="14" Foreground="#FF00C5FF" Click="BtnLogin_Click"/>
        <Label x:Name="lblWelcomeText" Content="Welcome back, " Margin="39,27,601,725" VerticalContentAlignment="Center" FontSize="18" FontWeight="Bold"/>
        <Label x:Name="lblWelcomeText_Copy" Content="First Name" VerticalContentAlignment="Center" FontSize="18" FontWeight="Bold" Margin="559,27,81,725"/>
        <Label x:Name="lblWelcomeText_Copy1" Content="What would you like to do today?" VerticalContentAlignment="Center" FontSize="18" FontWeight="Bold" Margin="266,256,234,496"/>
    </Grid>
</Page>

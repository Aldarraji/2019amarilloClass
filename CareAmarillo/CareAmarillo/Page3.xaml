<Window x:Class="CareAmarillo.Page3"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CareAmarillo"
        mc:Ignorable="d"
        Title="Page3" Height="800" Width="800">
    <Grid>
        <Button x:Name="btnRegister" Content="Register" HorizontalAlignment="Left" Margin="421,521,0,0" VerticalAlignment="Top" Width="108" Height="39" Background="#FF00C5FF" Foreground="White" FontSize="14" BorderBrush="#FF00F3FF" Click="BtnRegister_Click"/>
        <Button x:Name="btnLogin" Content="Login" HorizontalAlignment="Left" Margin="291,521,0,0" VerticalAlignment="Top" Width="113" BorderBrush="#FF00C5FF" Background="White" Height="39" FontSize="14" Foreground="#FF00C5FF" Click="BtnLogin_Click"/>
        <Button x:Name="btnChange" Content="Change Password" HorizontalAlignment="Left" Margin="337,585,0,0" VerticalAlignment="Top" Width="146" BorderBrush="White" Background="White" Foreground="#FF00C5FF" Click="BtnChange_Click"/>
        <Image x:Name="imgHands" HorizontalAlignment="Left" Height="197" Margin="303,-12,0,0" VerticalAlignment="Top" Width="262" Source="Capture.JPG" RenderTransformOrigin="0.643,0.319"/>
        <Label x:Name="lblCare" Content="CareAmarillo" HorizontalAlignment="Left" Margin="337,159,0,0" VerticalAlignment="Top" FontSize="24" Height="95" Width="146"/>
        <Label x:Name="lblRegister" Content="Register as an emergency shelter provider" HorizontalAlignment="Left" Margin="291,190,0,0" VerticalAlignment="Top" Width="238"/>
        <Label Content="Provider Name" HorizontalAlignment="Left" Height="34" Margin="291,231,0,0" VerticalAlignment="Top" Width="98"/>
        <Label x:Name="lblZipCode" Content="Zip Code" HorizontalAlignment="Left" Height="34" Margin="291,265,0,0" VerticalAlignment="Top" Width="69"/>
        <Label Content="Available Beds" HorizontalAlignment="Left" Height="34" Margin="291,299,0,0" VerticalAlignment="Top" Width="89"/>
        <Label Content="Email" HorizontalAlignment="Left" Height="34" Margin="291,333,0,0" VerticalAlignment="Top" Width="69"/>
        <Label Content="Address" HorizontalAlignment="Left" Height="34" Margin="291,367,0,0" VerticalAlignment="Top" Width="69"/>
        <Label Content="Phone Number" HorizontalAlignment="Left" Height="34" Margin="291,401,0,0" VerticalAlignment="Top" Width="98"/>
        <TextBox x:Name="txtZipCode" HorizontalAlignment="Left" Height="20" Margin="432,265,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="97" TextAlignment="Center"/>
        <TextBox x:Name="txtBed" HorizontalAlignment="Left" Height="20" Margin="432,299,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="97" TextAlignment="Center"/>
        <TextBox x:Name="txtEmail" HorizontalAlignment="Left" Height="20" Margin="432,333,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="97" TextAlignment="Center"/>
        <TextBox x:Name="txtAddress" HorizontalAlignment="Left" Height="20" Margin="432,367,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="97" TextAlignment="Center"/>
        <TextBox x:Name="txtPhone" HorizontalAlignment="Left" Height="20" Margin="432,401,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="97" TextAlignment="Center"/>
        <TextBox x:Name="txtPName" HorizontalAlignment="Left" Height="20" Margin="432,234,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="97" TextAlignment="Center"/>
        <Label x:Name="lblCity" Content="City" HorizontalAlignment="Left" Height="34" Margin="291,435,0,0" VerticalAlignment="Top" Width="98"/>
        <Label x:Name="lblState" Content="State" HorizontalAlignment="Left" Height="34" Margin="291,469,0,0" VerticalAlignment="Top" Width="98"/>
        <TextBox x:Name="txtCity" HorizontalAlignment="Left" Height="20" Margin="432,435,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="97" TextAlignment="Center"/>
        <TextBox x:Name="txtState" HorizontalAlignment="Left" Height="20" Margin="432,469,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="97" TextAlignment="Center"/>

    </Grid>
</Window>
